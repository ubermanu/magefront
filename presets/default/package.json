{
  "name": "magefront-preset-default",
  "version": "0.3.3",
  "type": "module",
  "keywords": [
    "magefront",
    "magento",
    "theme",
    "preset"
  ],
  "author": "Emmanuel Vodor <manu@paca.happy-dev.fr>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ubermanu/magefront.git",
    "directory": "presets/default"
  },
  "main": "dist/preset.cjs",
  "module": "dist/preset.mjs",
  "types": "dist/preset.d.ts",
