    "magefront-plugin-requirejs-config": "workspace:~",
    "magefront-plugin-terser": "workspace:~"
  },
  "peerDependencies": {
    "magefront": "workspace:^"
  },
  "exports": {
    ".": {
      "import": "./dist/preset.mjs",
      "require": "./dist/preset.cjs",
      "types": "./dist/preset.d.ts"
    }
