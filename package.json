{
  "name": "magefront",
  "version": "0.24.3",
  "type": "module",
  "description": "A modern Magento 2 theme builder",
  "keywords": [
    "magefront",
    "magento",
    "theme",
    "builder",
    "frontend",
    "less",
    "sass",
    "stylus"
  ],
  "author": "Emmanuel Vodor <manu@paca.happy-dev.fr>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ubermanu/magefront.git"
  },
  "main": "dist/magefront.cjs",
  "module": "dist/magefront.mjs",
  "types": "dist/magefront.d.ts",
  "scripts": {
    "changeset": "changeset",
    "ts-check": "tsc --noEmit",
    "build": "unbuild",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "browser-sync": "^2.29.3",
    "chokidar": "^3.5.3",
    "cli-table": "^0.3.11",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "kleur": "^4.1.5",
    "magefront-preset-default": "workspace:~",
    "memoizee": "^0.4.15",
    "pretty-ms": "^8.0.0",
    "sade": "^1.8.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/browser-sync": "^2.27.0",
    "@types/cli-table": "^0.3.2",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.4",
    "@types/memoizee": "^0.4.8",
    "@types/node": "^20.5.9",
    "@ubermanu/prettier-config": "^3.2.0",
    "dotenv": "^16.3.1",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "magefront": "dist/cli.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/magefront.mjs",
      "require": "./dist/magefront.cjs",
      "types": "./dist/magefront.d.ts"
    }
  }
}
