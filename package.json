{
  "name": "magefront",
  "version": "0.19.0",
  "type": "module",
  "keywords": [
    "magefront",
    "magento",
    "theme"
  ],
  "author": "Emmanuel Vodor <manu@paca.happy-dev.fr>",
  "license": "MIT",
  "files": [
    "dist",
    "types",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ubermanu/magefront.git"
  },
  "module": "dist/main.js",
  "types": "types/main.d.ts",
  "scripts": {
    "ts-check": "tsc --noEmit",
    "types": "rimraf types && tsc --declaration --emitDeclarationOnly --outDir types",
    "build": "rimraf dist && tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "prepublishOnly": "pnpm run build && pnpm run types"
  },
  "dependencies": {
    "browser-sync": "^2.27.7",
    "chokidar": "^3.5.3",
    "cli-table": "^0.3.11",
    "commander": "^8.3.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.1",
    "magefront-plugin-js-translation": "workspace:*",
    "magefront-plugin-less": "workspace:*",
    "magefront-plugin-requirejs-config": "workspace:*",
    "winston": "^3.8.2"
  },
  "optionalDependencies": {
    "magefront-plugin-babel": "workspace:*",
    "magefront-plugin-cssnano": "workspace:*",
    "magefront-plugin-postcss": "workspace:*",
    "magefront-plugin-react": "workspace:*",
    "magefront-plugin-sass": "workspace:*",
    "magefront-plugin-stylus": "workspace:*",
    "magefront-plugin-svelte": "workspace:*",
    "magefront-plugin-terser": "workspace:*",
    "magefront-plugin-typescript": "workspace:*"
  },
  "devDependencies": {
    "@types/browser-sync": "^2.26.3",
    "@types/cli-table": "^0.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.0.1",
    "@types/node": "^18.7.16",
    "dotenv": "^16.0.2",
    "jest": "^29.0.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.0",
    "typescript": "^4.8.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {},
    "moduleNameMapper": {
      "^magefront-plugin-less$": "<rootDir>/plugins/less/plugin.ts",
      "^magefront-plugin-requirejs-config$": "<rootDir>/plugins/requirejs-config/plugin.ts",
      "^magefront-plugin-js-translation$": "<rootDir>/plugins/js-translation/plugin.ts"
    },
    "testMatch": [
      "**/tests/*.test.ts"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.ts"
    ]
  },
  "bin": "dist/cli.js"
}
