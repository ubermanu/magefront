{
  "name": "magefront",
  "version": "0.23.2",
  "type": "module",
  "description": "A modern Magento 2 theme builder",
  "keywords": [
    "magefront",
    "magento",
    "theme",
    "builder",
    "frontend",
    "less",
    "sass",
    "stylus"
  ],
  "author": "Emmanuel Vodor <manu@paca.happy-dev.fr>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ubermanu/magefront.git"
  },
  "main": "dist/magefront.cjs",
  "module": "dist/magefront.mjs",
  "types": "dist/magefront.d.ts",
  "scripts": {
    "ts-check": "tsc --noEmit",
    "build": "unbuild",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "browser-sync": "^2.29.3",
    "chokidar": "^3.5.3",
    "cli-table": "^0.3.11",
    "fast-glob": "^3.3.0",
    "fs-extra": "^10.1.0",
    "kleur": "^4.1.5",
    "magefront-preset-default": "workspace:~",
    "memoizee": "^0.4.15",
    "pretty-ms": "^8.0.0",
    "sade": "^1.8.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/browser-sync": "^2.27.0",
    "@types/cli-table": "^0.3.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.5.3",
    "@types/memoizee": "^0.4.8",
    "@types/node": "^18.16.19",
    "@ubermanu/prettier-config": "^1.7.1",
    "dotenv": "^16.3.1",
    "jest": "^29.6.1",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.0",
    "turbo": "^1.10.8",
    "typescript": "^4.9.5",
    "unbuild": "^1.2.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {},
    "moduleNameMapper": {
      "^magefront-plugin-less$": "<rootDir>/plugins/less/src/plugin.ts",
      "^magefront-plugin-requirejs-config$": "<rootDir>/plugins/requirejs-config/src/plugin.ts",
      "^magefront-plugin-js-translation$": "<rootDir>/plugins/js-translation/src/plugin.ts",
      "^magefront-preset-default$": "<rootDir>/presets/default/src/preset.ts"
    },
    "testMatch": [
      "**/tests/*.test.ts"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.ts"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "magefront": "dist/cli.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/magefront.mjs",
      "require": "./dist/magefront.cjs",
      "types": "./dist/magefront.d.ts"
    }
  }
}
